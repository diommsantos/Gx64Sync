    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "preLaunchTask": "Build and Load x64Dbg Debuggable Plugin",
            "processId":"${command:pickProcess}",
        },
        {
            "name": "Launch Test",
            "preLaunchTask": "Build Test",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tests/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "integratedTerminal"
        },


    ]
}